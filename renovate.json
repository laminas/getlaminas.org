{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>laminas/.github:renovate-config"
  ],
  "packageRules": [
    {
      "matchDepTypes": ["require"],
      "rangeStrategy": "bump"
    },
    {
      "matchPackageNames": ["php"],
      "rangeStrategy": "replace",
      "groupName": "PHP"
    }
  ]
}
